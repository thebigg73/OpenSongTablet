                android:id="@+id/action_settingsCCLI_to_CCLILogFragment"
                app:destination="@id/settingsCCLILog" />
        </fragment>
        <fragment
            android:id="@+id/settingsCCLILog"
            android:name="com.garethevans.church.opensongtablet.ccli.CCLILogFragment"
            android:label="settingsCCLILog"
            tools:layout="@layout/settings_ccli_log"/>

        <!-- Display graph -->
        <navigation
            android:id="@+id/display_graph"
            app:startDestination="@+id/displayMenuFragment">

            <!-- Display fragment -->
            <fragment
                android:id="@+id/displayMenuFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.DisplayMenuFragment"
                android:label="settings_display"
                tools:layout="@layout/settings_display">
                <action
                    android:id="@+id/action_displayMenuFragment_to_themeSetupFragment"
                    app:destination="@id/themeSetupFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_fontSetupFragment"
                    app:destination="@id/fontSetupFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_actionBarSettingsFragment"
                    app:destination="@id/actionBarSettingsFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_menuSettingsFragment"
                    app:destination="@id/menuSettingsFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_displayExtraFragment"
                    app:destination="@id/displayExtraFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_displayScalingFragment"
                    app:destination="@id/displayScalingFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_connectedDisplayFragment"
                    app:destination="@id/connectedDisplayFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_inlineSetFragment"
                    app:destination="@id/inlineSetFragment" />
                <action
                    android:id="@+id/action_displayMenuFragment_to_marginsFragment"
                    app:destination="@id/marginsFragment" />
            </fragment>

            <!-- Theme fragment -->
            <fragment
                android:id="@+id/themeSetupFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.ThemeSetupFragment"
                android:label="settings_theme"
                tools:layout="@layout/settings_theme">
                <deepLink
                    android:id="@+id/deepLinkTheme"
                    app:uri="@string/deeplink_theme" />
            </fragment>

            <!-- Colour chooser -->
            <dialog
                android:id="@+id/chooseColorBottomSheet"
                android:name="com.garethevans.church.opensongtablet.screensetup.ChooseColorBottomSheet"
                android:label="display_color_settings"
                tools:layout="@layout/bottom_sheet_choose_color" />

            <!-- Font chooser -->
            <fragment
                android:id="@+id/fontSetupFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.FontSetupFragment"
                android:label="settings_fonts"
                tools:layout="@layout/settings_fonts">
                <action
                    android:id="@+id/action_fontSetupFragment_to_fontSetupPreviewFragment"
                    app:destination="@id/fontSetupPreviewFragment" />
                <deepLink
                    android:id="@+id/deepLinkFonts"
                    app:uri="@string/deeplink_fonts" />
            </fragment>

            <!-- Font preview -->
            <fragment
                android:id="@+id/fontSetupPreviewFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.FontSetupPreviewFragment"
                android:label="settings_fonts_preview"
                tools:layout="@layout/settings_fonts_preview" />
            <fragment
                android:id="@+id/actionBarSettingsFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.ActionBarSettingsFragment"
                android:label="settings_actionbar"
                tools:layout="@layout/settings_actionbar">
                <deepLink
                    android:id="@+id/deepLinkActionBar"
                    app:uri="@string/deeplink_actionbar" />
            </fragment>
            <fragment
                android:id="@+id/menuSettingsFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.MenuSettingsFragment"
                android:label="MenuSettingsFragment"
                tools:layout="@layout/settings_menu"/>
            <fragment
                android:id="@+id/displayExtraFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.DisplayExtraFragment"
                android:label="DisplayExtraFragment"
                tools:layout="@layout/settings_display_extra"/>
            <fragment
                android:id="@+id/displayScalingFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.DisplayScalingFragment"
                android:label="DisplayScalingFragment"
                tools:layout="@layout/settings_display_scaling">
                <deepLink
                    android:id="@+id/deepLinkScaling"
                    app:uri="@string/deeplink_scaling" />
            </fragment>

            <!-- Connected display -->
            <fragment
                android:id="@+id/connectedDisplayFragment"
                android:name="com.garethevans.church.opensongtablet.secondarydisplay.SecondaryDisplaySettingsFragment"
                android:label="settings_connected_display"
                tools:layout="@layout/settings_display_connected">
                <deepLink
                    android:id="@+id/deepLinkConnectedDisplay"
                    app:uri="@string/deeplink_connected_display" />
            </fragment>
            <fragment
                android:id="@+id/inlineSetFragment"
                android:name="com.garethevans.church.opensongtablet.setmenu.InlineSetFragment"
                android:label="settings_display_inline_set"
                tools:layout="@layout/settings_sets_inline">
                <deepLink
                    android:id="@+id/deepLinkInlineSet"
                    app:uri="@string/deeplink_inlineset" />
            </fragment>
            <fragment
                android:id="@+id/marginsFragment"
                android:name="com.garethevans.church.opensongtablet.screensetup.MarginsFragment"
                android:label="settings_margins"
                tools:layout="@layout/settings_margins">
                <deepLink
                    android:id="@+id/deepLinkMargins"
                    app:uri="@string/deeplink_margins" />
            </fragment>

        </navigation>

        <!-- Midi -->
        <fragment
            android:id="@+id/midiFragment"
            android:name="com.garethevans.church.opensongtablet.midi.MidiFragment"
            android:label="settings_midi"
            tools:layout="@layout/settings_midi">
            <deepLink
                android:id="@+id/deepLinkMidi"
                app:uri="@string/deeplink_midi" />
        </fragment>

        <!-- Profiles -->
        <fragment
            android:id="@+id/profileFragment"
            android:name="com.garethevans.church.opensongtablet.preferences.ProfileFragment"
            android:label="settings_profiles"
            tools:layout="@layout/settings_profiles">
            <deepLink
                android:id="@+id/deepLinkProfiles"
                app:uri="@string/deeplink_profiles" />
        </fragment>

        <!-- Controls graph -->
        <navigation
            android:id="@+id/control_graph"
            app:startDestination="@id/controlMenuFragment">

            <!-- Control options -->
            <fragment
                android:id="@+id/controlMenuFragment"
                android:name="com.garethevans.church.opensongtablet.controls.ControlMenuFragment"
                android:label="settings_control"
                tools:layout="@layout/settings_control">
                <action
                    android:id="@+id/action_controlMenuFragment_to_pedalsFragment"
                    app:destination="@id/pedalsFragment" />
                <deepLink
                    android:id="@+id/deepLinkControls"
                    app:uri="@string/deeplink_controls" />
                <action
                    android:id="@+id/action_controlMenuFragment_to_pageButtonFragment"
                    app:destination="@id/pageButtonFragment" />
                <action
                    android:id="@+id/action_controlMenuFragment_to_gesturesFragment"
                    app:destination="@id/gesturesFragment" />
                <action
                    android:id="@+id/action_controlMenuFragment_to_swipeFragment"
                    app:destination="@id/swipeFragment" />
            </fragment>

            <!-- Pedal settings -->
            <fragment
                android:id="@+id/pedalsFragment"
                android:name="com.garethevans.church.opensongtablet.controls.PedalsFragment"
                android:label="settings_pedal"
                tools:layout="@layout/settings_pedal">
                <deepLink
                    android:id="@+id/deepLinkPedals"
                    app:uri="@string/deeplink_pedals" />
            </fragment>
            <fragment
                android:id="@+id/pageButtonFragment"
                android:name="com.garethevans.church.opensongtablet.controls.PageButtonFragment"
                android:label="PageButtonFragment"
                tools:layout="@layout/settings_pagebuttons">
                <deepLink
                    android:id="@+id/deepLinkPageButtons"
                    app:uri="@string/deeplink_page_buttons" />
            </fragment>
            <fragment
                android:id="@+id/gesturesFragment"
                android:name="com.garethevans.church.opensongtablet.controls.GesturesFragment"
                android:label="GesturesFragment"
                tools:layout="@layout/settings_gestures">
                <deepLink
                    android:id="@+id/deepLinkGestures"
                    app:uri="@string/deeplink_gestures" />
            </fragment>
            <fragment
                android:id="@+id/swipeFragment"
                android:name="com.garethevans.church.opensongtablet.controls.SwipeFragment"
                android:label="SwipeFragment"
                tools:layout="@layout/settings_swipes">
                <deepLink
                    android:id="@+id/deepLinkSwipe"
                    app:uri="@string/deeplink_swipe" />
            </fragment>

        </navigation>

        <!-- About graph -->
        <navigation
            android:id="@+id/about_graph"
            app:startDestination="@id/aboutAppFragment">

            <!-- About options -->
            <fragment
                android:id="@+id/aboutAppFragment"
                android:name="com.garethevans.church.opensongtablet.appdata.AboutAppFragment"
                android:label="settings_about"
                tools:layout="@layout/settings_about">
                <action
                    android:id="@+id/action_aboutAppFragment_to_languageFragment"
                    app:destination="@id/languageFragment" />
            </fragment>

            <!-- Language -->
            <fragment
                android:id="@+id/languageFragment"
                android:name="com.garethevans.church.opensongtablet.appdata.LanguageFragment"
                android:label="settings_language"
                tools:layout="@layout/settings_language" />

        </navigation>
        <deepLink
            android:id="@+id/deepLinkPreferences"
            app:uri="@string/deeplink_preferences" />


        <dialog
            android:id="@+id/alertInfoDialogFragment"
            android:name="com.garethevans.church.opensongtablet.appdata.AlertInfoBottomSheet"
            android:label="@string/information">
            <deepLink
                android:id="@+id/deepLinkAlert"
                app:uri="@string/deeplink_alert" />
        </dialog>

        <!--Song actions graph -->
        <navigation android:id="@+id/actions_graph"
            app:startDestination="@id/songActionsMenuFragment">

            <fragment
                android:id="@+id/songActionsMenuFragment"
                android:name="com.garethevans.church.opensongtablet.songprocessing.SongActionsMenuFragment"
                android:label="SongActionsMenuFragment"
                tools:layout="@layout/settings_songactions">
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_editSongFragment"
                    app:destination="@+id/editsong_graph" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_highlighterFragment"
                    app:destination="@id/highlighterFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_musicScoreFragment"
                    app:destination="@id/musicScoreFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_stickyNotesFragment"
                    app:destination="@id/stickyNotesFragment" />
                <deepLink
                    android:id="@+id/deepLinkSongActions"
                    app:uri="@string/deeplink_song_actions" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_exportFragment"
                    app:destination="@id/exportFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_metronomeFragment"
                    app:destination="@id/metronomeFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_linksFragment"
                    app:destination="@id/linksFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_chords_graph"
                    app:destination="@id/chords_graph" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_pads_graph"
                    app:destination="@id/pads_graph" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_import_graph"
                    app:destination="@id/import_graph" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_autoscrollSettingsFragment"
                    app:destination="@id/autoscrollSettingsFragment" />
                <action
                    android:id="@+id/action_songActionsMenuFragment_to_bulkTagAssignFragment"
                    app:destination="@id/bulkTagAssignFragment" />
            </fragment>
            <fragment
                android:id="@+id/highlighterFragment"
                android:name="com.garethevans.church.opensongtablet.highlighter.HighlighterFragment"
                android:label="HighlighterFragment"
                tools:layout="@layout/settings_highlighter">
                <action
                    android:id="@+id/action_highlighterFragment_to_highlighterEditFragment"
                    app:destination="@id/highlighterEditFragment" />
            </fragment>
            <fragment
                android:id="@+id/highlighterEditFragment"
                android:name="com.garethevans.church.opensongtablet.highlighter.HighlighterEditFragment"
                android:label="HighlighterEditFragment"
                tools:layout="@layout/settings_highlighter_edit">
                <deepLink
                    android:id="@+id/deepLinkHighlighter"
                    app:uri="@string/deeplink_highlighter" />
            </fragment>
            <fragment
                android:id="@+id/musicScoreFragment"
                android:name="com.garethevans.church.opensongtablet.abcnotation.MusicScoreFragment"
                android:label="MusicScoreFragment"
                tools:layout="@layout/settings_abcnotation">
                <deepLink
                    android:id="@+id/deepLinkABC"
                    app:uri="@string/deeplink_abc" />
            </fragment>
            <fragment
                android:id="@+id/stickyNotesFragment"
                android:name="com.garethevans.church.opensongtablet.stickynotes.StickyNotesFragment"
                android:label="StickyNotesFragment"
                tools:layout="@layout/settings_stickynotes">
                <deepLink
                    android:id="@+id/deepLinkStickyNotes"
                    app:uri="@string/deeplink_sticky_notes" />
            </fragment>
            <fragment
                android:id="@+id/exportFragment"
                android:name="com.garethevans.church.opensongtablet.export.ExportFragment"
                android:label="ExportFragment"
                tools:layout="@layout/settings_export">
                <deepLink
                    android:id="@+id/deepLinkExport"
                    app:uri="@string/deeplink_export" />
            </fragment>
            <fragment
                android:id="@+id/metronomeFragment"
                android:name="com.garethevans.church.opensongtablet.metronome.MetronomeFragment"
                android:label="MetronomeFragment"
                tools:layout="@layout/settings_metronome">
                <deepLink
                    android:id="@+id/deepLinkMetronome"
                    app:uri="@string/deeplink_metronome" />
            </fragment>
            <fragment
                android:id="@+id/linksFragment"
                android:name="com.garethevans.church.opensongtablet.links.LinksFragment"
                android:label="LinksFragment"
                tools:layout="@layout/settings_links">
                <deepLink
                    android:id="@+id/deepLinkLinks"
                    app:uri="@string/deeplink_links" />
                <action
                    android:id="@+id/action_linksFragment_to_linksBottomSheet"
                    app:destination="@id/linksBottomSheet" />
            </fragment>
            <dialog
                android:id="@+id/linksBottomSheet"
                android:name="com.garethevans.church.opensongtablet.links.LinksBottomSheet"
                android:label="LinksBottomSheet"
                tools:layout="@layout/bottom_sheet_links"/>
            <navigation android:id="@+id/chords_graph"
                app:startDestination="@id/chordSettingsFragment">
                <fragment
                    android:id="@+id/chordSettingsFragment"
                    android:name="com.garethevans.church.opensongtablet.chords.ChordSettingsFragment"
                    android:label="ChordSettingsFragment"
                    tools:layout="@layout/settings_chords">
                    <action
                        android:id="@+id/action_chordSettingsFragment_to_chordFormatFragment"
                        app:destination="@id/chordFormatFragment" />
                    <action
