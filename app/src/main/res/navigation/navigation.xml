        android:name="com.garethevans.church.opensongtablet.presenter.PresenterFragment"
        android:label="fragment_presenter"
        tools:layout="@layout/mode_presenter">
        <action
            android:id="@+id/action_presenterFragment_to_preferences_graph"
            app:destination="@id/preferences_graph" />
        <action
            android:id="@+id/action_presenterFragment_to_editsong_graph"
            app:destination="@id/editsong_graph" />
        <action
            android:id="@+id/action_presenterFragment_to_import_graph"
            app:destination="@id/import_graph" />
        <deepLink
            android:id="@+id/deepLinkPresenter"
            app:uri="@string/deeplink_presenter" />
        <action
            android:id="@+id/action_presenterFragment_to_alertInfoDialogFragment"
            app:destination="@id/alertInfoDialogFragment" />
    </fragment>

    <!-- The preferences graph -->
    <navigation
        android:id="@+id/preferences_graph"
        app:startDestination="@id/preferencesFragment">

        <!-- The main preferences fragment -->
