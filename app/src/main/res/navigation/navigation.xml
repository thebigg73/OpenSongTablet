                        app:destination="@id/customChordsFragment" />
                    <deepLink
                        android:id="@+id/deepLinkChords"
                        app:uri="@string/deeplink_chords" />
                </fragment>
                <fragment
                    android:id="@+id/chordFormatFragment"
                    android:name="com.garethevans.church.opensongtablet.chords.ChordFormatFragment"
                    android:label="ChordFormatFragment"
                    tools:layout="@layout/settings_chords_format">
                <deepLink
                    android:id="@+id/deepLinkChordSettings"
                    app:uri="@string/deeplink_chords_settings" />
                </fragment>
                <fragment
                    android:id="@+id/customChordsFragment"
                    android:name="com.garethevans.church.opensongtablet.chords.CustomChordsFragment"
                    android:label="CustomChordsFragment"
                    tools:layout="@layout/settings_chords_custom">
                    <deepLink
                        android:id="@+id/deepLinkChordsCustom"
                        app:uri="@string/deeplink_chords_custom" />
                </fragment>

            </navigation>
            <navigation android:id="@+id/pads_graph"
                app:startDestination="@id/padSettingsFragment">
                <fragment
                    android:id="@+id/padSettingsFragment"
                    android:name="com.garethevans.church.opensongtablet.pads.PadSettingsFragment"
